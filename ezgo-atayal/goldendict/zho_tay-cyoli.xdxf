							<def>
				<deftext>cbaʼiy</deftext>
				<ex>
					<ex_orig>你教我織布好嗎？</ex_orig>
					<ex_tran>cbaʼiy cu cminun ga?</ex_tran>
				</ex>
			</def>			<def>
				<deftext>cnbaʼan</deftext>
			</def>			</def>
